{
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "./docs"
    ],
    "verbose": true,
    "events": {
      "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
    },
      "env": {
        "NODE_ENV": "",
        "PORT": 9000,
        "PROJECT_DIR": "./",
        "DEV_ENV": "local_dev"
      },
    "watch": ["src"],
    "ext": "ts",
    "exec": "ts-node ./src/indexNode.ts"
}